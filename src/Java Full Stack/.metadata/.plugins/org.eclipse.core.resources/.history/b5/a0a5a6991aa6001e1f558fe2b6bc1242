
public class LogicalOperator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean a=false;
		boolean b=true;
		System.out.println("a&&b = "+(a&&b));
		System.out.println("a||b = "+(a||b));
		System.out.println("!a= "+(!a));
		System.out.println("!b= "+(!b));
		
	}

}
